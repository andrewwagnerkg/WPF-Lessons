<Application x:Class="WpfLessons.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfLessons"
             xmlns:system="clr-namespace:System;assembly=System.Runtime"
             StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>
        <system:String x:Key="Exit">Exit</system:String>
        <system:String x:Key="File">File</system:String>
        <system:String x:Key="About">About</system:String>
    </Application.Resources>
</Application>
